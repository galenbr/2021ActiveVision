<?xml version="1.0" encoding="utf-8"?>
<robot name="gripper">
  <gazebo>
      <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
        <robotNamespace>/gripper</robotNamespace>
        <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        <legacyModeNS>true</legacyModeNS>
      </plugin>
  </gazebo>

  <gazebo reference="L1">
    <mu1>1</mu1>
    <mu2>1</mu2>
    <kp>10000000</kp>
    <kd>1</kd>
    <minDepth>0.001</minDepth>
  </gazebo>

  <gazebo reference="L2">
    <mu1>1</mu1>
    <mu2>1</mu2>
    <kp>10000000</kp>
    <kd>1</kd>
    <minDepth>0.001</minDepth>
  </gazebo>

  <link name="world">
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </link>

  <joint name="fixed"  type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <link
    name="base_link">
    <inertial>
      <origin
        xyz="6.6061E-06 6.4177E-05 0.027251"
        rpy="0 0 0" />
      <mass
        value="0.15337" />
      <inertia
        ixx="0.0001038"
        ixy="5.6961E-07"
        ixz="-6.9342E-13"
        iyy="7.4038E-05"
        iyz="-4.4165E-08"
        izz="0.00017386" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://gripper/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.75294 0.75294 0.75294 1" />
      </material>
    </visual>
  </link>

  <link
    name="L1">
    <inertial>
      <origin
        xyz="1.7149E-05 0.0014078 0.049755"
        rpy="0 0 0" />
      <mass
        value="0.037478" />
      <inertia
        ixx="3.7231E-05"
        ixy="4.835E-07"
        ixz="3.5543E-07"
        iyy="3.6319E-05"
        iyz="-3.9016E-06"
        izz="8.1669E-06" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://gripper/meshes/L1.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.75294 0.75294 0.75294 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://gripper/meshes/L1.STL" />
      </geometry>
    </collision>
  </link>

<!-- right finger -->
  <joint
    name="J1"
    type="continuous">
    <origin
      xyz="0 -0.02985 0.06936"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="L1" />
    <axis
      xyz="1 0 0" />
    <dynamics damping="0.01" friction="0.01"/>
    <limit
      lower="0"
      upper="0"
      effort="10"
      velocity="0.5" />
      <hardwareInterface>EffortJointInterface</hardwareInterface>
  </joint>



  <link
    name="L2">
    <inertial>
      <origin
        xyz="-0.000917152694428269 0.00358803500603186 0.0496456329588647"
        rpy="0 0 0" />
      <mass
        value="0.0374781252555018" />
      <inertia
        ixx="3.72312944777091E-05"
        ixy="4.4542620900761E-07"
        ixz="-4.02118046472307E-07"
        iyy="3.68148926584893E-05"
        iyz="1.01465713209036E-06"
        izz="7.67152143751456E-06" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://gripper/meshes/L1.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://gripper/meshes/L1.STL" />
      </geometry>
    </collision>
  </link>

<!-- left finger -->
  <joint
    name="J2"
    type="continuous">
    <origin
      xyz="0 0.02985 0.06936"
      rpy="0 0 3.1415" />
    <parent
      link="base_link" />
    <child
      link="L2" />
    <axis
      xyz="1 0 0" />
    <dynamics damping="0.01" friction="0.01"/>
    <limit
      lower="0"
      upper="0"
      effort="10"
      velocity="0.5" />
      <hardwareInterface>EffortJointInterface</hardwareInterface>
  </joint>

  <transmission name="head_swivel_trans1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="J1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="head_swivel_motor1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="head_swivel_trans2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="J2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="head_swivel_motor2">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <gazebo>
    <plugin name="friction_plugin" filename="libfriction_plugin.so"> </plugin>
    <!-- <low_friction>1e-1</low_friction>
    <high_friction>1e5</high_friction> -->
  </gazebo>

</robot>
